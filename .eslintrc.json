{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "overrides": [
    {
      "files": "webpack.*.config.js",
      "rules": {
        "import/no-extraneous-dependencies": "off"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {}
}
